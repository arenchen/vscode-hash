{
    "name": "hash",
    "displayName": "hash",
    "description": "Get a Hash Code",
    "version": "0.0.2",
    "license": "MIT",
    "publisher": "netcorext",
    "author": {
        "name": "Aren Chen",
        "email": "gucci313031@gmail.com"
    },
    "homepage": "https://github.com/arenchen/vscode-hash",
    "icon": "images/icon.png",
    "bugs": {
        "url": "https://github.com/arenchen/vscode-hash/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/arenchen/vscode-hash.git"
    },
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "Hash",
        "Checksum"
    ],
    "main": "./out/src/extension",
    "activationEvents": [
        "onCommand:hash.crc32",
        "onCommand:hash.md5",
        "onCommand:hash.sha1",
        "onCommand:hash.sha256",
        "onCommand:hash.sha512",
        "onCommand:hash.crc32.notify",
        "onCommand:hash.md5.notify",
        "onCommand:hash.sha1.notify",
        "onCommand:hash.sha256.notify",
        "onCommand:hash.sha512.notify"
    ],
    "contributes": {
        "commands": [
            {
                "command": "hash.crc32",
                "title": "Copy CRC32 Hash"
            },
            {
                "command": "hash.md5",
                "title": "Copy MD5 Hash"
            },
            {
                "command": "hash.sha1",
                "title": "Copy SHA1 Hash"
            },
            {
                "command": "hash.sha256",
                "title": "Copy SHA256 Hash"
            },
            {
                "command": "hash.sha512",
                "title": "Copy SHA512 Hash"
            },
            {
                "command": "hash.crc32.notify",
                "title": "Show CRC32 Hash"
            },
            {
                "command": "hash.md5.notify",
                "title": "Show MD5 Hash"
            },
            {
                "command": "hash.sha1.notify",
                "title": "Show SHA1 Hash"
            },
            {
                "command": "hash.sha256.notify",
                "title": "Show SHA256 Hash"
            },
            {
                "command": "hash.sha512.notify",
                "title": "Show SHA512 Hash"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorHasSelection",
                    "command": "hash.crc32",
                    "group": "hash"
                },
                {
                    "when": "editorHasSelection",
                    "command": "hash.md5",
                    "group": "hash"
                },
                {
                    "when": "editorHasSelection",
                    "command": "hash.sha1",
                    "group": "hash"
                },
                {
                    "when": "editorHasSelection",
                    "command": "hash.sha256",
                    "group": "hash"
                },
                {
                    "when": "editorHasSelection",
                    "command": "hash.sha512",
                    "group": "hash"
                }
            ]
        },
        "scripts": {
            "vscode:prepublish": "npm run compile",
            "compile": "tsc -p ./",
            "watch": "tsc -watch -p ./",
            "postinstall": "node ./node_modules/vscode/bin/install",
            "test": "npm run compile && node ./node_modules/vscode/bin/test"
        },
        "devDependencies": {
            "typescript": "^2.6.1",
            "vscode": "^1.1.21",
            "tslint": "^5.8.0",
            "@types/node": "^8.10.25",
            "@types/mocha": "^2.2.42"
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/copy-paste": "^1.1.30",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "copy-paste": "^1.3.0",
        "crypto": "^1.0.1"
    }
}
